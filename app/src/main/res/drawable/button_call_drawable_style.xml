<?xml version="1.0" encoding="utf-8"?>

<!-- Attribute drawableTint is only used in API level 23 and higher (current min is 19) -->

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_enabled="true"
        android:drawable="@drawable/ic_call_on"/>

    <item
        android:state_enabled="false"
        android:drawable="@drawable/ic_call_off"/>

</selector>